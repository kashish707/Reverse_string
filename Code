#include<stdio.h>
#include<conio.h>
#include<string.h>
int main()
{
	char a[100],b[100];
	printf("Enter :\n");
	gets(a);
	int i=0,j=0;
	printf("Word by Word Reverse is :\n");
	while(a[i]!='\0')
	{
		if(a[i]!=' ')
		{
			b[j]=a[i];
			j++;
		}
	    else
	    {
	    	b[j]='\0';
	     	printf("%s",strrev(b));
	    	j=0;
	    	printf(" ");
		}
	i++;
	}
	b[j]='\0';  // last word ke liye
	puts(strrev(b));
    return 0;
}
